{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Program",
			"program": "${workspaceFolder}/dist/index.js",
			"preLaunchTask": "build"
		},
		{
			"name": "Debug app",
			"type": "chrome",
			"request": "launch",
			"sourceMaps": true,
			"url": "http://localhost:3333",
			"webRoot": "${workspaceFolder}/www",
			"sourceMapPathOverrides": {
				"webpack:///*": "${webRoot}/build/*",
				"/./*": "${webRoot}/build/*",
				"/*": "${webRoot}/build/*",
				"src/*": "${webRoot}/../src/*",
				"./src/*": "${webRoot}/../src/*",
				"/src/*": "${webRoot}/../src/*",
				"../src/*": "${webRoot}/../../src/*",
				"./../src/*": "${webRoot}/../../src/*",
				"/../src/*": "${webRoot}/../../src/*",
				"~/src/*": "${webRoot}/../../src/*",
				"./~/*": "${webRoot}/../../src/*",
				"/~/*": "${webRoot}/../../src/*"
			}
		}
	]
}
